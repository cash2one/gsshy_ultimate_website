<SCRIPT type=text/javascript>
$(document).ready(function(){
$("ul#topnav li").hover(function(){
$(this).css({ 'background' : 'url(static/images/nav_lion.png) repeat-x'});
$(this).find("a").css({ 'color' : '#990000'});
$("ul#topnav li.home").css({ 'background' : 'none'});
$(this).find("span").show();
},function(){
$(this).css({ 'background' : 'none'});
$(this).find("a").css({ 'color' : '#ffffff'});
$(this).find("span").hide();
});
$("#nav li:eq(0)").css("margin-left","0px");
});
</SCRIPT>


